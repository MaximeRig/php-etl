{
    "name": "oliverde8/php-etl",
    "type": "library",
    "license": "MIT",
    "description": "A small etl coded in php, to extract/transform/load Edit with ease.",
    "require": {
        "php": ">=7.1.0",
        "oliverde8/associative-array-simplified": "^1.0",
        "psr/log": "^1.0",
        "symfony/expression-language": "^3.4|^4.3",
        "symfony/validator": "^2.7|^3.4|^4.3",
        "symfony/yaml": "^3.0|^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "config": {
        "platform": {
            "php": "7.2.0"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "Oliverde8\\": "src/Oliverde8/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {

        }
    }
}
